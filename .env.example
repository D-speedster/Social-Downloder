# Telegram Bot Configuration
BOT_TOKEN=your_main_bot_token_here

# Delivery Bot Configuration (for adult content)
DELIVERY_BOT_TOKEN=your_delivery_bot_token_here

# Telegram API Configuration
API_ID=your_api_id_here
API_HASH=your_api_hash_here

# Additional Settings
YOUTUBE_MAX_RETRIES=3
RAPIDAPI_KEY=your_rapidapi_key_here

# File Paths Configuration
COOKIE_FILE_PATH=./cookie.txt
FFMPEG_PATH=C:\ffmpeg\bin\ffmpeg.EXE

# Database Configuration
# Windows: C:\Users\YourUsername\Documents\Database
# Linux/Ubuntu: /var/lib/social-db/
# Default (relative): ./data/database
DB_BASE_PATH_WINDOWS=C:\Users\YourUsername\Documents\Database
DB_BASE_PATH_LINUX=/var/lib/social-db/
DB_BASE_PATH_DEFAULT=./data/database

# Proxy Configuration (uncomment and configure if needed for restricted networks)
# PROXY_HOST=127.0.0.1
# PROXY_PORT=1080
# PROXY_USERNAME=
# PROXY_PASSWORD=
